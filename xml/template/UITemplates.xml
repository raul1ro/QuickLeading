<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

    <Font name="QuickLeadingWhiteFont" font="Fonts\FRIZQT__.TTF" virtual="true" height="10">
        <Color r="1" g="1" b="1"/>
    </Font>

    <Frame name="QuickLeadingSimpleFrameTemplate" parent="" virtual="true" frameStrata="HIGH">
        <Layers>
            <Layer level="BACKGROUND" textureSubLevel="0">
                <Texture parentKey="Background">
                    <Color r="0.1" g="0.1" b="0.1" a="0"/>
                </Texture>
            </Layer>
            <!-- Top border -->
            <Layer level="BORDER">
                <Texture>
                    <Color r="0.75" g="0.75" b="0.75" a="1"/>
                    <Size y="1"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT" x="-1" y="1"/>
                        <Anchor point="TOPRIGHT" relativeTo="$parent" relativePoint="TOPRIGHT" x="1" y="1"/>
                    </Anchors>
                </Texture>
            </Layer>
            <!-- Right border -->
            <Layer level="BORDER">
                <Texture>
                    <Color r="0.75" g="0.75" b="0.75" a="0.75"/>
                    <Size y="1"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativeTo="$parent" relativePoint="TOPRIGHT" x="1" y="1"/>
                        <Anchor point="BOTTOMRIGHT" relativeTo="$parent" relativePoint="BOTTOMRIGHT" x="1" y="-1"/>
                    </Anchors>
                </Texture>
            </Layer>
            <!-- Bottom border -->
            <Layer level="BORDER">
                <Texture>
                    <Color r="0.75" g="0.75" b="0.75" a="0.75"/>
                    <Size y="1"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativeTo="$parent" relativePoint="BOTTOMLEFT" x="-1" y="-1"/>
                        <Anchor point="BOTTOMRIGHT" relativeTo="$parent" relativePoint="BOTTOMRIGHT" x="1" y="-1"/>
                    </Anchors>
                </Texture>
            </Layer>
            <!-- Left border -->
            <Layer level="BORDER">
                <Texture>
                    <Color r="0.75" g="0.75" b="0.75" a="0.75"/>
                    <Size y="1"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT" x="-1" y="1"/>
                        <Anchor point="BOTTOMLEFT" relativeTo="$parent" relativePoint="BOTTOMLEFT" x="-1" y="-1"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Frame>

</Ui>